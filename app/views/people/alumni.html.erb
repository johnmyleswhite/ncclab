<div style="width: 550px; margin-left: 80px; padding-top: 50px;">
  <h1>NCC LAB ALUMNI</h1>
  
  <% @alumni.each do |alumnus| %>
   <div>
     <% if alumnus.url.nil? %>
      <p><a class="tooltip" style="font-weight: bold; text-decoration: none; cursor: text;" rel="#"><%= alumnus.name %><span class="classic"><b><%= alumnus.title %></b><br /><%= alumnus.office %><br /><%= alumnus.email %><br /><b><%= alumnus.phone %></b><br /><%= alumnus.blurb %><br /><%= alumnus.image %></span></a><br /><%= alumnus.image %></p>
     <% else %>
      <p><a class="tooltip" style="font-weight: bold;" href="<%= alumnus.url %>" rel="#"><%= alumnus.name %><span class="classic"><b><%= alumnus.title %></b><br /><%= alumnus.office %><br /><%= alumnus.email %><br /><b><%= alumnus.phone %></b><br /><%= alumnus.blurb %><br /><%= alumnus.image %></span></a><br /><%= alumnus.image %></p>
  	 <% end%>
   </div>
  <% end%>
</div>
<div style="padding-bottom: 25px;">
</div>

