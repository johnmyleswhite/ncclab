<div style="padding-left: 80px; padding-top: 50px;">
  <h1>DIRECTORS</h1>
  <% @directors.each do |director| %>
   <div class="positionpage">
    <div class="personimage">
     <img src="<%= director.image2 %>" />
    </div>
    <div class="personinfo">
     <a class="personname" href="<%= director.url %>" name="<%= director.name %>"><%= director.name %></a><br /><%= director.title %><br /><%= director.institution %>
     <br />
     <br />
     <%= director.office %><br /><%= director.email %><br /><%= director.phone %>
     <br />
     <br />
     <%= director.blurb %>
    </div>
   </div>
  <% end%>
</div>

