<% @years.each do |year| %>
  <% if year.nil? %>
    <h2>In Press</h2>
  <% else %>
    <h2><%= year %></h2>
  <% end %>
  <% Publication.find_all_by_year(year).each do |publication| %>
    <p class="publication">
      <%= publication.authors %>
      (<%= publication.year_string %>)
      <%= publication.title %>
      <%= publication.title %>
      [<a href="<%= publication.file %>">PDF</a>]
    </p>
  <% end %>
<% end %>
